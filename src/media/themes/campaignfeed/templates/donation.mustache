<div id="template">
	<div class="feed-item-cover">
		<div class="date">{{created_day}} </br> {{created_date_month}}</div>
		<div class="feed-item">
			<div class="feed-item-inner">
				<span><i class="fa fa-dollar fa-lg"></i></span>
				<span><img class="feed-item-image" src="{{actor.image.avatar}}"></span>
				<span><a href='{{actor.url}}'>{{actor.name}}</a> donated {{object.amount}} to the campaign</span>
				<hr />
			</div>
		</div>
	</div>
</div>
