<div class="col-xs-12 col-sm-12 col-lg-12">
	<span><i class="fa fa-image fa-lg"></i></span>&nbsp;
	<span><img class="feed-item-image" src="{{actor.image.avatar}}" /></span>
	<span class="language en-GB"><a href='{{actor.url}}'>{{actor.name}}</a> added {{object.count}} image(s) to the campaign</span>
	<div>
		{{#object.url}}
			<img class="feed-item-image-upload" src="{{.}}" height="150" width="150"/>
		{{/object.url}}
	</div>
	<hr />
</div>
