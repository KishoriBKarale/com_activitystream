<div id="template">
	<div class="feed-item-cover">
		<div class="date">{{created_day}} </br> {{created_date_month}}</div>
		<div class="feed-item">
			<div class="feed-item-inner">
				<span><i class="fa fa-image fa-lg"></i></span>
				<span><img class="feed-item-image" src="{{actor.image.avatar}}"></span>
				<span><a href='{{actor.url}}'>{{actor.name}}</a> added {{object.count}} give-away(s) to the campaign</span>
				<div>
					{{#object.url}}
						<img class="feed-item-image-upload" src="{{.}}" height="150" width="150"/>
					{{/object.url}}
				</div>
				<hr />
			</div>
		</div>
	</div>
</div>
